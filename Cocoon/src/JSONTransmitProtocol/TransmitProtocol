			[
				{
					"username":<String>
					"solved":<int>
				}*
			] 
			"friendlist":
			[
				"username":<String>, *
			]
			"statistics": [JSON Array]
			[
				{
					"problemtype":<String>
					"solved":<int>
				}, *
			]
		}
	}
